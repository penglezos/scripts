<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- A custom remote to access github using SSH -->
  <remote fetch="ssh://git@github.com" name="ssh" review="review.arrowos.net" revision="arrow-12.1" />

  <!-- The raphael device tree -->
  <project name="penglezos/device_xiaomi_raphael" path="device/xiaomi/raphael" remote="ssh" />

  <!-- The raphael kernel -->
  <project name="penglezos/kernel_xiaomi_raphael" path="kernel/xiaomi/raphael" remote="ssh" />

  <!-- The vendor repository containing proprietary files required for raphael -->
  <project name="penglezos/vendor_xiaomi_raphael" path="vendor/xiaomi/raphael" remote="ssh" />

</manifest>
